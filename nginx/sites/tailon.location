
 # Tailon - Logs
 location /tailon/ {
    auth_basic           "Area de admin";
    auth_basic_user_file conf.d/.htpasswd;
    include conf.d/common.include;
    proxy_pass http://tailon:8082;
  }
  # Tailon - Proxy Web Socket
  location /tailon/ws {
    proxy_pass http://tailon:8082/tailon/ws;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "upgrade";
  }